@article{meijer2012,
  title = {Do Neighborhoods Affect Individual Mortality? {{A}} Systematic Review and Meta-Analysis of Multilevel Studies},
  shorttitle = {Do Neighborhoods Affect Individual Mortality?},
  author = {Meijer, Mathias and R{\"o}hl, Jeannette and Bloomfield, Kim and Grittner, Ulrike},
  year = {2012},
  month = apr,
  journal = {Social Science \& Medicine},
  volume = {74},
