  langid = {english},
  pmid = {31727643},
  keywords = {meta-analysis,neighbourhood socioeconomic status,obesity,obesogenic environment,overweight},
  file = {/Users/samharper/Dropbox/work/research/zotero/Mohammed-2019-Neighbourhood.pdf}
}

@article{nieuwenhuis2016,
